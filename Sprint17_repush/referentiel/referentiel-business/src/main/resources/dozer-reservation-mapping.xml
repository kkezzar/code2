		<!-- lieu -->
		<field>
			<a>idLieu</a>
			<b>refLieu.id</b>
		</field>
		<field>
			<a>designationLieu</a>
			<b>refLieu.designation</b>
		</field>
		<field>
			<a>designationArabeLieu</a>
			<b>refLieu.designationArabe</b>
		</field>
		<!-- Structure -->
		<field>
			<a>idStructure</a>
			<b>refStructure.id</b>
		</field>
		<field>
			<a>llStructureLatin</a>
