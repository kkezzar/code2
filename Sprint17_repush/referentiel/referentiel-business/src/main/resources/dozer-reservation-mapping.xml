		<!-- Responsable -->
		<field>
			<a>idResponsable</a>
			<b>refIndividuByResponsable.id</b>
		</field>
		<field>
			<a>nomLatinResponsable</a>
			<b>refIndividuByResponsable.nomLatin</b>
		</field>
		<field>
			<a>prenomLatinResponsable</a>
			<b>refIndividuByResponsable.prenomLatin</b>
		</field>
		<field>
			<a>nomArabeResponsable</a>
			<b>refIndividuByResponsable.nomarabe</b>
		</field>
		<field>
			<a>prenomArabeResponsable</a>
			<b>refIndividuByResponsable.prenomArabe</b>
		</field>

	</mapping>


	<mapping map-null="false">
		<class-a>dz.gov.mesrs.sii.referentiel.business.model.dto.RefReservationDto
		</class-a>
		<class-b>dz.gov.mesrs.sii.commons.data.model.referentiel.RefSituation
		</class-b>
		<field-exclude>
			<a>id</a>
			<b>id</b>
		</field-exclude>
		<field>
			<a>llSituationFr</a>
			<b>refEntiteSituation.nomenclature.libelleLongFr</b>
		</field>
		<field>
			<a>llSituationAr</a>
			<b>refEntiteSituation.nomenclature.libelleLongAr</b>
		</field>
