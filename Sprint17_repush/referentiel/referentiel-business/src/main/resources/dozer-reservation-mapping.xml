		</field>
		<!-- Individu -->
		<field>
			<a>idIndividu</a>
			<b>refIndividu.id</b>
		</field>
		<field>
			<a>nomLatin</a>
			<b>refIndividu.nomLatin</b>
		</field>
		<field>
			<a>prenomLatin</a>
			<b>refIndividu.prenomLatin</b>
		</field>
		<field>
			<a>nomArabe</a>
