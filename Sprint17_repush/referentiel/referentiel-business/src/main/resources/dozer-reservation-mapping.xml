		</field>
		<field>
			<a>prenomArabe</a>
			<b>refIndividu.prenomArabe</b>
		</field>
		<!-- Groupe -->
		<field>
			<a>idGroupe</a>
			<b>refGroupe.id</b>
		</field>
		<field>
			<a>llGroupe</a>
			<b>refGroupe.llGroupe</b>
		</field>
		<field>
			<a>llGroupeArabe</a>
			<b>refGroupe.llGroupeArabe</b>
		</field>

		<!-- Equipement -->
		<field>
			<a>idEquipement</a>
			<b>refEquipement.id</b>
		</field>
		<field>
			<a>designationEquipement</a>
			<b>refEquipement.designation</b>
